{
  "name": "{connectorName}",
  "remoteServers": ["{applicationUrl1}", "{applicationUrl2}"],
  "remoteUserNames": ["{ideUsername}", "{ideOrganization/IdeUsername}", "{ideUrl@ideUsername}"],
  "datamodels": [
    {
      "name": "{datamodelName}",
      "class": "Postgres",
      "connectionOptions": {
        "host": "{hostname}",
        "database": "{databaseName}",
        "user": "{username}",
        "password": "{password}"
      }
    },
    {
      "name": "{datamodelName}",
      "class": "SQLServer",
      "connectionOptions": {
        "server": "{hostname}",
        "database": "{databaseName}",
        "user": "{username}",
        "password": "{password}"
      }
    },
    {
      "name": "{datamodelName}",
      "class": "Oracle",
      "maxRows": 10000,
      "connectionOptions": {
        "connectString": "{hostname/databaseName}",
        "user": "{username}",
        "password": "{password}"
      }
    }
  ]
}
